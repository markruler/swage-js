{
  "name": "swage",
  "version": "0.4.0",
  "description": "CLI tool for converting Swagger API specification JSON to Excel",
  "main": "./dist/main.js",
  "bin": {
    "swage": "./dist/main.js"
  },
  "scripts": {
    "test": "npx jest --config jest.config.json",
    "build": "babel src/ --out-dir dist/",
    "watch": "babel src/ --out-dir dist/ --watch",
    "start": "# node --experimental-json-modules src/main.js # 'type': 'module'",
    "dist": "./dist/main.js --help",
    "local": "npm install --global .",
    "clean": "rm -rf dist/ && npm uninstall --global ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markruler/swage.git"
  },
  "keywords": [
    "swagger",
    "json",
    "excel"
  ],
  "author": {
    "name": "Changsu Im",
    "email": "csu0414@gmail.com",
    "url": "https://github.com/markruler"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markruler/swage/issues"
  },
  "homepage": "https://github.com/markruler/swage#readme",
  "dependencies": {
    "excel4node": "^1.7.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@types/jest": "^26.0.19",
    "babel-preset-minify": "^0.5.1",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "jshint": "^2.12.0",
    "prettier": "^2.2.1"
  }
}
